{
  "name": "electron-tdd-project",
  "version": "1.0.0",
  "description": "Electron application built with TDD principles",
  "main": "./src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "NODE_ENV=development electron-forge start",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "docs": "jsdoc -c jsdoc.config.js",
    "docs:dev": "nodemon --watch src -e js --exec 'npm run docs'"
  },
  "keywords": [
    "electron",
    "tdd"
  ],
  "author": "",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_tdd_project"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0",
    "@electron-forge/maker-deb": "^6.0.0",
    "@electron-forge/maker-rpm": "^6.0.0",
    "@electron-forge/maker-squirrel": "^6.0.0",
    "@electron-forge/maker-zip": "^6.0.0",
    "@types/jest": "^29.0.0",
    "clean-jsdoc-theme": "^4.3.0",
    "electron": "^25.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-jest": "^27.0.0",
    "jest": "^29.0.0",
    "jsdoc": "^4.0.4",
    "nodemon": "^3.1.7",
    "prettier": "^2.0.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
